@{
    ViewBag.Title = "Home Page";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    @Scripts.Render("~/bundles/jquery")
    <title>Quick Test</title>
    <style>
        body {
        }

        #viewbox {
            overflow: hidden;
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0px;
            left: 0px;
            background-color: transparent;
        }

        #background-video {
            overflow: hidden;
            z-index: -1;
            position: absolute;
            margin: auto;
            top: 0px;
            width: 100%;
            height: 100%;
            background-color: rgba(0,0,0,.9);
        }

        #sphere-box {
            z-index: 2000;
            overflow: hidden;
            background-color: transparent;
            position: absolute;
            top: 0px;
            left: 0px;
            height: 100%;
            width: 100%; 
        }

        #sphere-box2 {
            z-index: 2000;
            overflow: hidden;
            background-color: transparent;
            position: absolute;
            top: 0px;
            left: 0px;
            height: 100%;
            width: 100%;
        }

        #heads-up-display {
            z-index: 4000;
            margin: auto;
            top: 0px;
            width: 99.1%;
            height: 99.1%;
            overflow: hidden;
            position: absolute;
            background-color: transparent;
        }

            #heads-up-display > div {
                background-color: rgba(255,0,0,0);
                width: 33%;
                height: 20%;
            }

        div#hud-top-left {
            position: absolute;
            top: 0px;
            left: 0px;
            background-color: transparent;
        }

        div#hud-top-center {
            position: absolute;
            top: 0px;
            left: 33.3%;
            background-color: transparent;
        }

        div#hudCoordInfo, div#hudOrientInfo, div#addTargetinfo, .helptext {
            /*text-shadow: 0em 0em 0.1em #8F7;*/
            color:#ff0000;
            background-color: transparent;
        }

        div#addTargetinfo {
            position: absolute;
            left: 0;
            bottom: 0;
        }

        div#hudCoordInfo, div#hudOrientInfo, .helptext {
            position: relative;
        }

        #hud-top-right {
            position: absolute;
            top: 0px;
            left: 66.6%;
        }

        div#hud-center-center {
            position: absolute;
            top: 33.3%;
            left: 33.3%;
            height: 33.3%;
            width: 33.3%;
            vertical-align: middle;
            background-color: rgba(255,0,0,0.0);
        }

            div#hud-center-center div.crosshair {
                position: absolute;
                width: 18px;
                height: 18px;
                position: absolute;
                top: 50%;
                left: 50%;
                margin-left: -9px;
                margin-top: -9px;
                text-align: center;
                font-weight: 700;
                font-size: 2em;
                text-shadow: 0em 0em 0.1em #8F7;
                overflow: visible;
                background-color: transparent;
            }

        div#hud-bottom-left {
            position: absolute;
            height: 20%;
            bottom: 0px;
            left: 0px;
            background-color: transparent;
        }

        #hud-bottom-center {
            position: absolute;
            left: 33.3%;
            height: 20%;
            bottom: 0px;
            width: 33.3%;
        }

        #hud-bottom-right {
            position: absolute;
            height: 20%;
            bottom: 0px;
            left: 66.6%;
            width: 33%;
        }

        div#compass-slider div#compass-indicator {
            top: 0px;
            width: 1px;
            margin-left: auto;
            margin-right: auto;
            z-index: 4001;
        }

        svg#compass-main {
            position: absolute;
            top: 0px;
            width: 100%;
        }

        div#compass-slider {
            width: 100%;
        }

        #map {
            height: 150px;
        }
        canvas#indicators {
            height:100%;
            width:100%;
            position:absolute;
            top:0;
            left:0;
        }
        .indicator {
            position:absolute;
            opacity: 0.4;
            color:#ff0000;
        }
    </style>
